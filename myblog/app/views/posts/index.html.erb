<h2>
    <%= link_to 'Add new', new_post_path, class: 'header-menu' %>
    My Post
</h2>
<ul>
<% @posts.each do |post| %>
    <li>
        <%= link_to post.title, post_path(post) %>
        <%= link_to '[編集]', edit_post_path(post), class:'command' %> <!-- linkテキストをEditにした-->
        <%= link_to '[削除]',
        post_path(post), 
        method: :delete, 
        class:'command',
        data: { confirm: 'Sure?' }
        %> 
    </li>
    <% end %>
</ul>